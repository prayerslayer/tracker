{
  "name": "Tracker",
  "version": "0.1",
  "description": "Tracks mouse + keyboard.",
  "background_page": "bg.html",
  "browser_action": {
    "default_title": "",
    "default_icon": "icon19.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["jquery.min.js", "tracker.js"]
    }
  ],
  "permissions": [
    "tabs"
  ]
}